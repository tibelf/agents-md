{
  "name": "@askchiefleo/agents-md",
  "version": "1.0.1",
  "description": "Generate compressed docs index for AGENTS.md (Vue, uni-app, etc.)",
  "bin": {
    "agents-md": "./bin/agents-md.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "bin",
    "dist",
    "rules"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/cli.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "execa": "^9.0.0",
    "fs-extra": "^11.0.0",
    "minimatch": "^10.1.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "agents",
    "claude",
    "ai",
    "docs",
    "vue",
    "uniapp",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kipper/agents-md"
  },
  "license": "MIT",
  "author": "Kipper"
}
